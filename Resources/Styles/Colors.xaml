<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Page background-->
    <Color x:Key="Main">#0B0E14</Color>

    <!--Frames/Cards-->
    <Color x:Key="Sections">#1A1C26</Color>

    <!--Buttons/Border Color-->
    <Color x:Key="Button">#2D324A</Color>

    <!--Primary Text-->
    <Color x:Key="StarWhite">#F8FAFC</Color>
    <!--SecondaryText-->
    <Color x:Key="OrbitGray">#94A3B8</Color>

    <!--Highlights-->
    <Color x:Key="NovaPurple">#A855F7</Color>
    
</ResourceDictionary>